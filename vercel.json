{
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "main.py"
      }
    ],
    "functions": {
      "**": {
        "excludeFiles": "{.next,*.cache,node_modules,public,app}/**"
      }
    }  
}
  