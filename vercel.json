{
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "main.py"
      }
    ],
    "functions": {
      "api/**": {
        "excludeFiles": "{.next,*.cache,node_modules,public,app}/**"
      }
    }  
  }
  